{
  "alumno": {
    "main": ["persona"],
    "nf+":["tramo_ingreso"],
    "noAdmin":["tramo_ingreso"]
  },

  "asignatura": {
    "main": ["nombre"],
    "unique": ["nombre"]
  },

  "calendario": {
    "orderDefault": {"anio":"DESC", "semestre":"DESC"},
    "main": ["descripcion","anio", "semestre"],
    "unique": ["descripcion"]
  },

  "cargo": {
    "main": ["descripcion"]
  },

  "centro_educativo": {
    "main": ["nombre"]
  },

  "comision": {
    "main": ["sede", "division", "calendario",  "planificacion"]
  },

  "curso": {
    "main": ["asignatura", "comision"]
  },

  "dia": {
    "orderDefault": {"numero":"asc"},
    "main": ["dia"]
  },

  "disposicion": {
    "main": ["asignatura", "planificacion"]
  },

  "domicilio": {
    "main": ["calle", "numero", "barrio"]
  },

  "file": {
    "main": ["name"]
  },

  "modalidad": {
    "main": ["nombre"]
  },

  "persona": {
    "main": ["nombres","apellidos","numero_documento"]
  },

  "plan": {
    "main": ["orientacion", "distribucion_horaria"],
    "uniqueMultiple": ["orientacion", "distribucion_horaria"]
  },

  "planificacion": {
    "main":["anio","semestre","plan"],
    "uniqueMultiple": ["plan", "anio", "semestre"],
    "nf+":["tramo"]
  },

  "planilla_docente": {
    "main":  ["numero"],
    "unique": ["id", "numero"]
  },

  "resolucion": {
    "main": ["numero"]
  },

  "sede": {
    "main": ["numero", "nombre"],
    "orderDefault": {"numero_pad":"asc"},
    "uniqueMultiple":["numero", "centro_educativo"]
  },

  "tipo_sede": {
    "main": ["descripcion"]
  }

}
